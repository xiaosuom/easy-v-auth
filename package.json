{
  "name": "easy-v-auth",
  "description": "Vue自定义指令v-auth",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "author": "zxz <953001542@qq.com> (https://zxz.cool)",
  "license": "MIT",
  "homepage": "https://github.com/xiaosuom/easy-auth-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaosuom/easy-auth-vue"
  },
  "keywords": [
